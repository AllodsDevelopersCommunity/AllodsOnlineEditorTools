		<multiline>false</multiline>
		<wrapText>false</wrapText>
		<showClippedSymbol>true</showClippedSymbol>
		<showClippedLine>true</showClippedLine>
		<ellipsis>false</ellipsis>
	</TextStyle>
	<minWidth>32</minWidth>
	<maxWidth>280</maxWidth>
</WidgetTextView>
