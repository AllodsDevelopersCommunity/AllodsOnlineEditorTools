<?xml version="1.0" encoding="UTF-8" ?>
<WidgetTextView>
	<Name>SmartLine</Name>
	<BackLayer href="/Interface/Ingame/ContextTooltip/ContextTooltipTextHighlight.(WidgetLayerTiledTexture).xdb#xpointer(/WidgetLayerTiledTexture)" />
	<FrontLayer href="" />
	<Placement>
		<X>
			<Sizing>WIDGET_SIZING_INTERNAL</Sizing>
			<Size>0</Size>
		</X>
		<Y>
			<Sizing>WIDGET_SIZING_INTERNAL</Sizing>
			<Size>0</Size>
		</Y>
	</Placement>
	<Visible>true</Visible>
	<FormatFileRef href="FormatDefault.txt" />
